        <section class="section section__contact" id="contact">
            <div class="container">
                <h2 class="section-heading" style="color: var(--color-blue);"><small style="color: var(--color-grey);">Get in Touch</small>Contact us for more information</h2>
                <div class="contact__form-container">
                    <form id="contactForm" action="/api/frontend/contact-us?formToken={{ formToken ("/api/frontend/contact-us") }}" method="post">
                        <div class="form-group--two-col">
                            <div class="form-group">
                                <label for="userName">FULL NAME *</label>
                                <input type="text" id="userName" name="userName" required placeholder="Enter your full name" class="form-control">
                                <span id="userNameError" class="validation-tag"></span>
                            </div>

                            <div class="form-group">
                                <label for="userEmail">EMAIL *</label>
                                <input type="email" id="userEmail" name="userEmail" required placeholder="Enter your Email" class="form-control">
                                <span id="userEmailError" class="validation-tag"></span>
                            </div>
                        </div>

                        <div class="form-group--two-col">
                            <div class="form-group">
                                <label for="userTel">PHONE</label>
                                <input id="userTel" name="userTel" placeholder="Enter your phone number" class="form-control" minlength="5" maxlength="16">
                                <span id="userTelError" class="validation-tag"></span>
                            </div>

                            <div class="form-group">
                                <label for="userCompany">COMPANY</label>
                                <input type="text" id="userCompany" name="userCompany" placeholder="Enter your company name" class="form-control">
                                <span id="userCompanyError" class="validation-tag"></span>
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="userMessage">MESSAGE *</label>
                            <textarea type="text" id="userMessage" name="userMessage" cols="5" rows="5" required placeholder="Type your message" class="form-control"></textarea>
                            <span id="userMessageError" class="validation-tag"></span>
                        </div>

                        <button type="submit" onclick="validateForm()" id="submitButton" disabled>Send Message</button>
                    </form>
                    <script>
                        let fields = [
                            {id: "userName", label: "Full name", required: true},
                            {id: "userEmail", label: "Email address", required: true},
                            {id: "userTel", label: "Phone number", required: false},
                            {id: "userCompany", label: "Company", required: false},
                            {id: "userMessage", label: "Message", required: true}
                        ];

                        window.validateForm = function() {
                            let isValid = true;

                            fields.forEach(field => {
                                let value = document.getElementById(field.id).value.trim();
                                let errorElement = document.getElementById(field.id + "Error");
                                let inputElement = document.getElementById(field.id);

                                if (field.required && !value) {
                                    errorElement.innerText = field.label + " is required.";
                                    inputElement.classList.add("error");
                                    isValid = false;
                                } else {
                                    errorElement.innerText = "";
                                    inputElement.classList.remove("error");
                                }
                            });

                            let email = document.getElementById("userEmail").value;
                            if (email && !email.match(/^\S+@\S+\.\S+$/)) {
                                document.getElementById("userEmailError").innerText = "Please enter a valid email address.";
                                document.getElementById("userEmail").classList.add("error");
                                isValid = false;
                            }

                            let phone = document.getElementById("userTel").value;
                            if (phone && !phone.match(/^\+?[0-9]\d{1,14}$/)) {
                                document.getElementById("userTelError").innerText = "Please enter a valid phone number.";
                                document.getElementById("userTel").classList.add("error");
                                isValid = false;
                            }

                            document.getElementById('submitButton').disabled = !isValid;

                            return isValid;
                        };

                        document.getElementById('contactForm').addEventListener('input', window.validateForm);
                        window.validateForm();
                    </script>

