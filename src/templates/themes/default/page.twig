    {% block headers %}
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Arimo:wght@400;700&family=Dosis:wght@400;600;700&display=swap" rel="stylesheet">
        <link rel="stylesheet" type="text/css" href="{{ Theme.Dir(site.theme) }}/css/reboot.css">
        <link rel="stylesheet" type="text/css" href="{{ Theme.Dir(site.theme) }}/css/tooltip.css">
        <link rel="stylesheet" type="text/css" href="{{ Theme.Dir(site.theme) }}/css/tiny-slider.css">
        <link rel="stylesheet" type="text/css" href="{{ Theme.Dir(site.theme) }}/css/default.css">
        <script src="{{ Theme.Dir(site.theme) }}/js/main.js"></script>
        <script src="{{ Theme.Dir(site.theme) }}/js/masonry.min.js"></script>
        <script src="{{ Theme.Dir(site.theme) }}/js/tiny-slider.min.js"></script>
        <script src="{{ Theme.Dir(site.theme) }}/js/imagesloaded.min.js"></script>

        <link rel="apple-touch-icon" sizes="180x180" href="{{ Theme.Dir(site.theme) }}/apple-touch-icon.png">
        <link rel="icon" type="image/png" sizes="32x32" href="{{ Theme.Dir(site.theme) }}/favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="16x16" href="{{ Theme.Dir(site.theme) }}/favicon-16x16.png">
        <link rel="manifest" href="{{ Theme.Dir(site.theme) }}/site.webmanifest">
        <link rel="mask-icon" href="{{ Theme.Dir(site.theme) }}/safari-pinned-tab.svg" color="#16b2cf">
        <meta name="msapplication-TileColor" content="#16b2cf">
        <meta name="theme-color" content="#16B2CF">

    {% endblock %}
    {% if site.custom %}
        {{ site.custom | raw }}
    {% endif %}
</head>
{% block body %}

